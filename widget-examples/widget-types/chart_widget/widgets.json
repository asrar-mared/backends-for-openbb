{
  "chains_plotly": {
    "name": "Chains chart example Plotly with raw data",
    "description": "Get current TVL of all chains and plot it with Plotly",
    "category": "Crypto",
    "type": "chart",
    "endpoint": "chains",
    "raw": true,
    "gridData": {
      "w": 40,
      "h": 9
    }
  },
  "chains_highchart": {
    "name": "Chains chart example Highchart",
    "description": "Get current TVL of all chains and plot it with Highchart",
    "category": "Crypto",
    "type": "chart-highcharts",
    "endpoint": "chains-highchart",
    "gridData": {
      "w": 40,
      "h": 9
    }
  },
  "chains_table": {
    "name": "Chains Chart example",
    "description": "A built in chart widget example",
    "category": "Crypto",
    "endpoint": "chains_table",
    "gridData": {
      "w": 20,
      "h": 9
    },
    "data": {
      "table": {
        "chartView": {
          "enabled": true,
          "chartType": "bar"
        },
        "showAll": false,
        "columnsDefs": [
          {
            "headerName": "Name",
            "field": "name",
            "chartDataType": "category"
          },
          {
            "headerName": "Total Value Locked",
            "field": "tvl"
          },
          {
            "headerName": "Token Symbol",
            "field": "tokenSymbol",
            "chartDataType": "category"
          }
        ]
      }
    }
  }
}